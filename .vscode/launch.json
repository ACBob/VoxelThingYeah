{
	"configurations": [
		{
			"name": "(gdb) Launch Client",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/VoxelThingYeah_client",
			"args": [
				"username VScodePlayer; fov 90"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
